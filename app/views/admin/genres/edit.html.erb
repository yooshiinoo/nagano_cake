<div class="container">
  <h2>ジャンル編集</h2>
  <div class="row">
    <div class="col-md-10">
      <%= form_with model: @genre, url:admin_genre_path do |f| %>
        <div class="form-group">
          <%= f.label "ジャンル名" %>
          <%= f.text_field :name, placeholder:"@genre.name" %>
          <%= f.submit "変更を保存", class:"btn btn-success" %>
        </div>
      <% end %>
    </div>
  </div>
</div>